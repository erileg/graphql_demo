type Movie {
    id: ID!
    title: String!
    director: String!
    releaseDate: String
}

extend type Query {
    findAllMovies: [Movie]!
    countMovies: Long!
}

extend type Mutation {
    newMovie(id: ID!, title: String!, director: String!, realeaseDate: String) : Movie!
    deleteMovie(id: ID!) : Boolean
}
